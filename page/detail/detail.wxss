/**************************浮动购物车*********************/
.fdgwc{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 94rpx;
  background: #ffffff;
  border-top: 2rpx solid #e0e0e0;
  z-index: 999;
}
.fdgwc_1{
  float:left;
  width:115rpx;
  height: 82rpx;
  text-align:center;
  color:#565656;
  background: #fff;
  font-size: 24rpx;
  padding-top: 12rpx;
}
.fdgwc_1 image{
  width: 40rpx;
  height:40rpx;
}
.fdgwc_2{
  position: relative;
  float:left;
  width:115rpx;
  height: 80rpx;
  text-align:center;
  color:#565656;
  background: #fff;
  font-size: 24rpx;
  padding-top: 14rpx;
}
.fdgwc_2 image{
  width: 36rpx;
  height:36rpx;
  padding-bottom: 4rpx;
}
.fdgwc_2 button{
  position: absolute;
  width:115rpx;
  height: 80rpx;
  top: 0rpx;
  left: 0rpx;
  opacity: 0;
  z-index: 2;
}
.fdgwc_3{
  float:right;
  width:390rpx;
  height: 80rpx;
  line-height: 94rpx;
  text-align:center;
  color:#fff;
  font-size: 30rpx;
  margin-right: 12rpx;
  padding-top: 14rpx;
}
.fdgwc_3 view{
  width: 100%;
  height: 66rpx;
  line-height: 66rpx;
  text-align: center;
  background: #37d15f;
  background: -webkit-linear-gradient(left,#32df89,#13b754);
  border-radius: 33rpx;
}
.fdgwc_3_off{
  float:right;
  width:390rpx;
  height: 80rpx;
  line-height: 94rpx;
  text-align:center;
  color:#fff;
  font-size: 30rpx;
  margin-right: 12rpx;
  padding-top: 14rpx;
}
.fdgwc_3_off view{
  width: 100%;
  height: 66rpx;
  line-height: 66rpx;
  text-align: center;
  background: #828282;
  background: -webkit-linear-gradient(left,#b0afaf,#828282);
  border-radius: 33rpx;
}
