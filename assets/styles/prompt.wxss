.prompt-container {
    display: flex;
    width: 100%;
    height: 80%;
    align-items: center;
    justify-content: center;
    text-align: center;
    color: #939393;
}

.prompt-message {
    margin-top: 0;
    text-align: center;
}

.prompt-message .prompt-icon {
    width: 230rpx;
    height: 230rpx;
}

.prompt-message .prompt-title {
    font-size: 28rpx;
    font-weight: bold;
    color: #818282;
    margin: 2px 0;
}

.prompt-message .prompt-text {
    font-size: 26rpx;
    margin: 2px 0;
}